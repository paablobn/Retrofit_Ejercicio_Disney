<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VerPersonajeActivity">

    <ImageView
        android:id="@+id/imgVerPersonaje"
        android:layout_width="240dp"
        android:layout_height="187dp"
        android:layout_marginTop="55dp"
        android:layout_marginBottom="47dp"
        app:layout_constraintBottom_toTopOf="@+id/lbNombreVerPersonaje"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/lbNombreVerPersonaje"
        android:layout_width="321dp"
        android:layout_height="22dp"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="409dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgVerPersonaje" />

    <TextView
        android:id="@+id/lbFilmsVerPersonaje"
        android:layout_width="321dp"
        android:layout_height="20dp"
        android:layout_marginTop="55dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbNombreVerPersonaje" />
</androidx.constraintlayout.widget.ConstraintLayout>